{
    "arrowParens": "avoid",
    "useTabs": true,
    "singleQuote": true,
    "tabWidth": 2,
    "semi": false,
    "trailingComma": "none",
    "importOrder":[
        "<THIRD_PARTY_MODULES>",
        "^@/core/(.*)$",
        "^@/core/component/(.*)$",
        "^@/components/(.*)$",
        "^@/component/layout(.*)$",
        "^@/component/screens(.*)$",
        "^@/component/ui/(.*)$",
        "^@/styles/(.*)$",
        "^@/config/(.*)$",
        "^@/utils/(.*)$",
        "^@/services/(.*)$",
        "^@/store/(.*)$",
        "^@/api/(.*)$",
        "^.+\\.(template\\.html|module\\.scss)$",
        "^../(.*)",
        "^./(.*)"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers":true
}